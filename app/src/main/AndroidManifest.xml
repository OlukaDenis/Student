<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mcdenny.student">

    <application
        android:allowBackup="true"
        android:icon="@drawable/images"
        android:label="@string/app_name"
        android:roundIcon="@drawable/images"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/activity_main_header"></activity>
        <activity
            android:name=".CourseActivity"
            android:label="@string/activity_course_header"></activity>
        <activity
            android:name=".AboutActivity"
            android:label="@string/activity_about_header"></activity>
        <activity
            android:name=".Staff"
            android:label="@string/activity_staff_header"></activity>
        <activity android:name=".StudentDetails"></activity>
    </application>

</manifest>